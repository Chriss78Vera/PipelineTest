{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
  }
  